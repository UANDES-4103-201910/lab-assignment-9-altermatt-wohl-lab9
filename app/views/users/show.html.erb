<div class="container">
	<div class="text-right">
		<button id="LogOut" type="submit" class="btn btn-default btn-lg" role="button"> <%= link_to 'Log out', log_out_path %> </button>		
	</div>
	<div class="jumbotron">
		<p id="notice"><%= notice %></p>
		<h1><%= @user.name + @user.last_name %> </h2>
		<h2><%= @user.email %></h2>
		<h2><%= @user.phone %></h2>
		<h2><%= @user.password %></h2>	
		<div class="btn-group btn-group-lg">
			<button id="Edit" type="submit" class="btn btn-default btn-lg" role="button"><%= link_to 'Edit', edit_user_path(@user) %></button>
			<button id="Back" type="submit" class="btn btn-default btn-lg" role="button"><%= link_to 'Back', users_path %></button>
		</div>	
	</div>
</div>