<div class="container">
	<div class="text-right">
		<button id="LogOut" type="submit" class="btn btn-default btn-lg" role="button"> <%= link_to 'Log out', log_out_path %> </button>		
	</div>
	<div class="jumbotron">
		<div class="caption">
			<h1>Editing User</h1>		
		</div>
		<%= render 'form', user: @user %>
		<div class="btn-group btn-group-lg">
			<button id="Show" type="submit" class="btn btn-default btn-lg" role="button"> <%= link_to 'Show', @user %> </button>
			<button id="Back" type="submit" class="btn btn-default btn-lg" role="button"><%= link_to 'Back', users_path %></button>
		</div>			
	</div>
</div>
